@article{RN19,
   author = {Yoon, S. J. and Pak, K. and Nam, T. and Yoon, A. and Kim, H. and Im, S. G. and Cho, B. J.},
   title = {Surface-Localized Sealing of Porous Ultralow-k Dielectric Films with Ultrathin (<2 nm) Polymer Coating},
   journal = {ACS Nano},
   volume = {11},
   number = {8},
   pages = {7841-7847},
   note = {Yoon, Seong Jun
Pak, Kwanyong
Nam, Taewook
Yoon, Alexander
