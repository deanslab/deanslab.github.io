   author = {Oh, J. G. and Pak, K. and Kim, C. S. and Bong, J. H. and Hwang, W. S. and Im, S. G. and Cho, B. J.},
   title = {A High-Performance Top-Gated Graphene Field-Effect Transistor with Excellent Flexibility Enabled by an iCVD Copolymer Gate Dielectric},
   journal = {Small},
   volume = {14},
   number = {9},
   note = {Oh, Joong Gun
