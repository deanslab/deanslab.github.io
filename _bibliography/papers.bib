@article{RN8,
   author = {Kim, M. J. and Pak, K. and Hwang, W. S. and Im, S. G. and Cho, B. J.},
   title = {Novel Vapor-Phase Synthesis of Flexible, Homogeneous Organic-Inorganic Hybrid Gate Dielectric with sub 5 nm Equivalent Oxide Thickness},
   journal = {ACS Appl Mater Interfaces},
   volume = {10},
   number = {43},
   pages = {37326-37334},
   note = {Kim, Min Ju
Pak, Kwanyong
