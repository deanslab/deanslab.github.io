@article{RN23,
   author = {Choi, J. and Lee, C. and Lee, C. and Park, H. and Lee, S. M. and Kim, C. H. and Yoo, H. and Im, S. G.},
   title = {Vertically stacked, low-voltage organic ternary logic circuits including nonvolatile floating-gate memory transistors},
   journal = {Nat Commun},
   volume = {13},
   number = {1},
   pages = {2305},
   note = {Choi, Junhwan
Lee, Changhyeon
Lee, Chungryeol
